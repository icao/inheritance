  // TODO: Se esta corrigiendo archivos REACT(actualmente), VUE, CSS, SASS, HTML, italicas
  "tokenColors": [
    {
      "scope": "string",
      "settings": {
        "foreground": "#8dff8d"
      }
    },
    {
      "scope": "punctuation, constant.other.symbol",
      "settings": {
        "foreground": "#00d9ff"
      }
    },
    {
      "scope": "constant.character.escape, text.html constant.character.entity.named",
      "settings": {
        "foreground": "#bac7e4"
      }
    },
    {
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#ff5f8b"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#ffbf7a"
      }
    },
    {
      "scope": "variable, variable.parameter, support.variable, variable.language, support.constant, meta.definition.variable entity.name.function, meta.function-call.arguments",
      "settings": {
        "foreground": "#bac7e4"
      }
    },
    {
      "scope": "keyword.other",
      "settings": {
        "foreground": "#ffbf7a"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#ffbf7a"
      }
    },
    {
      "scope": "keyword, modifier, support.type.object, constant.language",
      "settings": {
        "foreground": "#00d9ff"
      }
    },
    {
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#7AA2F7"
      }
    },
    {
      "scope": "storage.type, storage.modifier, storage.control",
      "settings": {
        "foreground": "#de7df7"
      }
    },
    {
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#ff4579",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.type, constant.other.key",
      "settings": {
        "foreground": "#ffbf7a"
      }
    },
    {
      "scope": "entity.name.type, entity.other.inherited-class, entity.other",
      "settings": {
        "foreground": "#ffbf7a"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#d1dfff46",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment punctuation.definition.comment, string.quoted.docstring",
      "settings": {
        "foreground": "#d1dfff46",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#00d9ff"
      }
